{
	"name": "CORS app for profiling website",
	"version": "1.0.3",
	"manifest_version": 2,
	"description": "allow cross origin resource sharing for profiling website, adds ",
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "on.png",
		"default_title": "Allow-Control-Allow-Origin"
	},
"permissions": [
		"storage",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],
	"web_accessible_resources": [
		"on.png"
	]
}